#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/nkr24/Documents/git/Sberbank/gos/node_modules/.pnpm/solid-start@0.3.5_@solidjs+meta@0.28.6_@solidjs+router@0.8.3_solid-js@1.7.11_solid-start-node@0.3.5_vite@4.4.9/node_modules/solid-start/node_modules:/home/nkr24/Documents/git/Sberbank/gos/node_modules/.pnpm/solid-start@0.3.5_@solidjs+meta@0.28.6_@solidjs+router@0.8.3_solid-js@1.7.11_solid-start-node@0.3.5_vite@4.4.9/node_modules:/home/nkr24/Documents/git/Sberbank/gos/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/home/nkr24/Documents/git/Sberbank/gos/node_modules/.pnpm/solid-start@0.3.5_@solidjs+meta@0.28.6_@solidjs+router@0.8.3_solid-js@1.7.11_solid-start-node@0.3.5_vite@4.4.9/node_modules/solid-start/node_modules:/home/nkr24/Documents/git/Sberbank/gos/node_modules/.pnpm/solid-start@0.3.5_@solidjs+meta@0.28.6_@solidjs+router@0.8.3_solid-js@1.7.11_solid-start-node@0.3.5_vite@4.4.9/node_modules:/home/nkr24/Documents/git/Sberbank/gos/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../solid-start/bin.cjs" "$@"
else
  exec node  "$basedir/../solid-start/bin.cjs" "$@"
fi
